{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "model",
  "type": "object",
  "required": [
    "name",
    "capabilities",
    "msr",
    "description",
    "variabledim",
    "al",
    "al_info",
    "shocks",
    "variables"
  ],
  "additionalProperties": false,
  "if": {
    "properties": {
      "al": {
        "const": true
      }
    }
  },
  "then": {
    "properties": {
      "al_info": {
        "$ref": "#/properties/al_info/anyOf/0"
      }
    }
  },
  "else": {
    "properties": {
      "al_info": {
        "$ref": "#/properties/al_info/anyOf/1"
      }
    }
  },
  "properties": {
    "$schema": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "al": {
      "$ref": "common#/definitions/bool"
    },
    "al_info": {
      "anyOf": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "forwards": {
              "$ref": "common#/definitions/arrayOfStrings"
            },
            "states_long": {
              "$ref": "common#/definitions/arrayOfStrings"
            },
            "states_short": {
              "$ref": "common#/definitions/arrayOfStrings"
            }
          }
        }, {
          "type": "null"
        }
      ]
    },
    "variabledim": {
      "enum": [
        1,
        2
      ]
    },
    "msr": {
      "anyOf": [
        {
          "$ref": "common#/definitions/arrayOfCoefficients"
        },
        {
          "type": "null"
        }
      ]
    },
    "shocks": {
      "$ref": "common#/definitions/arrayOfNamesHumanReadableNames"
    },
    "variables": {
      "$ref": "common#/definitions/arrayOfNamesHumanReadableNames"
    },
    "capabilities": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "unconditional_variances"
      ],
      "properties": {
        "unconditional_variances": {
          "$ref": "common#/definitions/bool"
        },
        "rules": {
          "$ref": "common#/definitions/arrayOfStrings"
        }
      }
    },
    "description": {
      "$ref": "common#/definitions/description"
    }
  }
}
