{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "JSON Output generated by Macromodelbase (CMD_MMB.m)",
  "definitions": {
    "singleElement": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "model",
        "rule",
        "shock",
        "func",
        "outputvar",
        "values"
      ],
      "properties": {
        "model": {
          "type": "string"
        },
        "rule": {
          "type": "string"
        },
        "shock": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Fis",
                "Mon"
              ]
            },
            {
              "type": "null"
            }
          ]
        },
        "func": {
          "type": "string",
          "enum": [
            "IRF",
            "AC",
            "VAR"
          ]
        },
        "outputvar": {
          "type": "string",
          "enum": [
            "inflation",
            "interest",
            "output",
            "outputgap"
          ]
        },
        "values": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "oneOf": [
                  {
                    "type": "number"
                  },
                  {
                    "enum": [ "_Inf_" ]
                  }
                ]
              }
            },
            {
              "type": "null"
            },
            {
              "type": "number"
            }
          ]
        }
      }
    }
  },

  "oneOf": [
    {
      "type": "array",
      "items": {
        "$ref": "#/definitions/singleElement"
      }
    }
  ]
}

