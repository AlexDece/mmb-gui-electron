{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "common",
  "definitions": {
    "arrayOfStrings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "arrayOfNamesHumanReadableNames": {
      "type": "array",
      "additionalItems": false,
      "items": {
        "$ref": "#/definitions/nameAndHumanReadableName"
      }
    },
    "nameAndHumanReadableName": {
      "type": "object",
      "required": [
        "name",
        "human_readable"
      ],
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "human_readable": {
          "type": "string"
        }
      }
    },
    "arrayOfCoefficients": {
      "type": "array",
      "items": {
        "type": [
          "string",
          "number"
        ]
      },
      "minItems": 33,
      "maxItems": 33
    },
    "description": {
      "type": "object",
      "required": [
        "ac_ref",
        "paper_title",
        "journal",
        "replicants_name",
        "pub_date",
        "keywords",
        "description",
        "authors"
      ],
      "properties": {
        "ac_ref": {
          "type": "string"
        },
        "paper_title": {
          "type": "string"
        },
        "journal": {
          "type": [
            "string",
            "null"
          ]
        },
        "replicants_name": {
          "type": [
            "string",
            "null"
          ]
        },
        "pub_date": {
          "type": "string"
        },
        "keywords": {
          "type": "array"
        },
        "description": {
          "type": "string"
        },
        "authors": {
          "$ref": "#/definitions/arrayOfStrings"
        }
      }
    }
  }
}
